  var images = [];
          images.push('images/sidebar1.jpg');
          images.push('wp-content/uploads/2014/03/placeholder-2.jpg');
          var index = 0;
  var backstretchSettings = { fade: 750, duration:3500};
  var len = images.length;
  var totalDuration = (backstretchSettings.fade + backstretchSettings.duration);
  var timer = null;
  var rotate = function() {
      $('aside').backstretch(images[0], backstretchSettings);
      timer = setInterval(function() {
          index++;
