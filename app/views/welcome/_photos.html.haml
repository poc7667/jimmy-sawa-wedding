%section#photos.row
  %header.section-title-container
    %h2.section-title Photos
    %span.hearts
  .small-12.columns
    %ul.clearing-thumbs{"data-clearing" => ""}
      - (0..7).to_a.each  do |i|
        - img_path ="wedding_photos/#{i}.jpg"
        %li
          %a{:href => asset_path(img_path)}
            %img.attachment-full.wp-post-image{:src => asset_path(img_path)}/
            %span.overlay-label LOVE#{i}